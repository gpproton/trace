        }
      }
    },
    "manager": {
      "type": "project.v0",
      "path": "../Manager/Trace.Manager.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "ConnectionStrings__messaging": "{messaging.connectionString}",
        "ConnectionStrings__trace": "{trace.connectionString}",
        "CassandraPort": "{cassandraPort.value}",
        "CassandraUsername": "{cassandraUsername.value}",
        "CassandraPassword": "{cassandraPassword.value}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
