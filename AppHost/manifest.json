        "ConnectionStrings__cache": "{cache.connectionString}",
        "ConnectionStrings__messaging": "{messaging.connectionString}",
        "ConnectionStrings__trace": "{trace.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "service-navigation": {
      "type": "project.v0",
      "path": "../Service/Navigation/Trace.Service.Navigation.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "MessagingUser": "{MessagingUser.value}",
        "MessagingPass": "{MessagingPass.value}",
        "DbUser": "{DbUser.value}",
        "DbPass": "{DbPass.value}",
        "CassandraPort": "{CassandraPort.value}",
        "CassandraUser": "{CassandraUser.value}",
        "CassandraPass": "{CassandraPass.value}",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "ConnectionStrings__messaging": "{messaging.connectionString}",
        "ConnectionStrings__trace": "{trace.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "gateway": {
      "type": "project.v0",
      "path": "../Host/Gateway/Trace.Gateway.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "services__service-core__http__0": "{service-core.bindings.http.url}",
        "services__service-core__https__0": "{service-core.bindings.https.url}",
        "services__service-integration__http__0": "{service-integration.bindings.http.url}",
        "services__service-integration__https__0": "{service-integration.bindings.https.url}",
        "services__service-navigation__http__0": "{service-navigation.bindings.http.url}",
        "services__service-navigation__https__0": "{service-navigation.bindings.https.url}",
        "ConnectionStrings__cache": "{cache.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "frontend": {
      "type": "project.v0",
      "path": "../Host/Frontend/Trace.Frontend.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "services__gateway__http__0": "{gateway.bindings.http.url}",
        "services__gateway__https__0": "{gateway.bindings.https.url}",
        "services__geocoding": "https://nominatim.openstreetmap.org/",
        "services__routing": "https://valhalla.openstreetmap.de/",
        "ConnectionStrings__cache": "{cache.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "manager": {
      "type": "project.v0",
      "path": "../Manager/Trace.Manager.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "MessagingUser": "{MessagingUser.value}",
        "MessagingPass": "{MessagingPass.value}",
        "DbUser": "{DbUser.value}",
        "DbPass": "{DbPass.value}",
        "CassandraPort": "{CassandraPort.value}",
        "CassandraUser": "{CassandraUser.value}",
        "CassandraPass": "{CassandraPass.value}",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "ConnectionStrings__messaging": "{messaging.connectionString}",
        "ConnectionStrings__trace": "{trace.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "cache": {
      "type": "container.v0",
      "connectionString": "{cache.bindings.tcp.host}:{cache.bindings.tcp.port}",
      "image": "docker.io/redis/redis-stack-server:7.2.0-v10",
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 6379
        }
      }
    },
    "messaging": {
      "type": "container.v0",
      "connectionString": "amqp://{MessagingUser.value}:{MessagingPass.value}@{messaging.bindings.tcp.host}:{messaging.bindings.tcp.port}",
      "image": "rabbitmq:3",
      "env": {
        "RABBITMQ_DEFAULT_USER": "{MessagingUser.value}",
        "RABBITMQ_DEFAULT_PASS": "{MessagingPass.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5672
        }
      }
    },
    "db": {
      "type": "container.v0",
      "connectionString": "Host={db.bindings.tcp.host};Port={db.bindings.tcp.port};Username={DbUser.value};Password={DbPass.value}",
      "image": "postgis/postgis:15-3.3",
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "{DbUser.value}",
        "POSTGRES_PASSWORD": "{DbPass.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "trace": {
      "type": "value.v0",
      "connectionString": "{db.connectionString};Database=trace"
    },
    "warehouse": {
      "type": "container.v0",
      "image": "scylladb/scylla:5.4",
      "bindings": {
        "cassandra": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 9042
        },
        "thrift": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 9160
        }
      }
    }
  }
}