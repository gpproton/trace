{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    "DomainNames": [
      "demo-0.trace.ng",
      "demo-1.trace.ng"
    ],
    "EmailAddress": "dev@trace.ng"
  },
  "FrontendProxies": {
    "Routes": {
      "gateway": {
        "ClusterId": "gateway",
        "Match": {
          "Path": "/graphql/{**catch-all}"
        },
        "Transforms": [
          {
            "ResponseHeader": "Source",
            "Append": "YARP",
            "When": "Success"
          }
        ]
      },
      "geocoding": {
        "ClusterId": "geocoding",
        "Match": {
          "Path": "/api/geocoding/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/geocoding"
          }
        ]
      },
      "navigation": {
        "ClusterId": "routing",
        "Match": {
          "Path": "/api/routing/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/routing"
          }
        ]
      }
    },
    "Clusters": {
      "gateway": {
        "Destinations": {
          "gateway": {
            "Address": "http://service-gateway/",
            "Health": "http://service-gateway/readiness"
          }
        }
      },
      "geocoding": {
        "Destinations": {
          "geocoding": {
            "Address": "https://nominatim.openstreetmap.org/"
          }
        }
      },
      "routing": {
        "Destinations": {
          "routing": {
            "Address": "https://valhalla.openstreetmap.de/"
          }
        }
      }
    }
  }
}
