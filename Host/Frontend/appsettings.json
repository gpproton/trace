{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    "DomainNames": [
      "demo-one.trace.ng",
      "demo-two.trace.ng"
    ],
    "EmailAddress": "dev@trace.ng"
  },
  "FrontendProxies": {
    "Routes": {
      "gateway": {
        "ClusterId": "gateway",
        "Match": {
          "Path": "/api/service"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/service"
          }
        ]
      },
      "geocoding": {
        "ClusterId": "geocoding",
        "Match": {
          "Path": "/api/geocoding/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/geocoding"
          }
        ]
      },
      "navigation": {
        "ClusterId": "routing",
        "Match": {
          "Path": "/api/routing/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/routing"
          }
        ]
      }
    },
    "Clusters": {
      "gateway": {
        "Destinations": {
          "gateway": {
            "Address": "https://localhost:5000/graphql"
          }
        }
      },
      "geocoding": {
        "Destinations": {
          "geocoding": {
            "Address": "https://nominatim.openstreetmap.org/"
          }
        }
      },
      "routing": {
        "Destinations": {
          "routing": {
            "Address": "https://valhalla.openstreetmap.de/"
          }
        }
      }
    }
  }
}
